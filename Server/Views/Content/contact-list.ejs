<div class="container">
    <h1>Business Contacts List</h1>

    <div class="row">
        <div class="col">
            <table class="table table-striped table-bordered table-hover">
                <thead class="table-dark">
                    <tr>
                        <th scope="col" class="text-center">#</th>
                        <th scope="col" class="">Name</th>
                        <th scope="col" class="">Contact Number</th>
                        <th scope="col" class="">Email</th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="contactList">
                    <!--repeatable rows-->
                    <% for(let count = 0; count < bcontacts.length; count++){ %>
                        <tr>
                            <th scope="row" class="text-center"><%= count + 1%></th>
                            <td><%= bcontacts[count].Name %></td>
                            <td><%= bcontacts[count].Number %></td>
                            <td><%= bcontacts[count].Email %></td>
                            <td></td>
                            <td></td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>